{
  "name": "football-stuff",
  "version": "1.0.0",
  "description": "How to use create-react-app with a custom Node API on Heroku",
  "engines": {
    "node": "8.9.x"
  },
  "scripts": {
    "start": "node server",
    "heroku-postbuild": "cd react-ui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "start-react": "cd react-ui && npm start"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.17.1",
    "mongodb": "^3.6.3",
    "mongoose": "^5.7.5",
    "node-cron": "^1.2.1",
    "react-bootstrap-table": "^4.3.1",
    "react-router-bootstrap": "^0.24.4",
    "request": "^2.88.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rbrilhante/footballstuff.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {}
}
